{
  "name": "react-slicknav",
  "version": "0.0.1",
  "description": "ReactJs Node version jQuery SlickNav plugin ",
  "author": "Ravi Kiran Lingam",
  "main": "dist/js/Slicknav.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:uncompressed": "webpack --mode production --config webpack.config.js --progress --colors && node-sass src/sass/slicknav.scss dist/css/slicknav.css",
    "build:minified": "webpack --mode production --config webpack.config.js --progress --colors --minified && node-sass src/sass/slicknav.scss dist/css/slicknav.css",
    "build": "npm run build:minified"
  },
  "keywords": [
    "react",
    "react-dom",
    "Slicknav"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saburiinfor/node-slicknav.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.6.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "terser": "^4.8.0",
    "webpack-cli": "^3.3.12",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "sass-loader": "^8.0.0",
    "ssri": ">=8.0.1",
    "webpack": "^4.46.0"
  }
}
